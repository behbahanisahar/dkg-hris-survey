{
  "name": "dkg-hris-survey",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "craco start",
    "start-dev-server": "craco start",
    "build": "craco build",
    "test": "node ./bin/sp-rest-proxy.js test-interactive",
    "test-debug": "node ./bin/sp-rest-proxy.js test-non-interactive",
    "test-interactive": "craco test",
    "test-non-interactive": "cross-env CI=true craco --inspect test --no-cache --runInBand",
    "test-coverage": "node ./bin/sp-rest-proxy.js test-ci",
    "test-vscode": "craco test --ci --coverage --reporters=default",
    "test-ci": "craco test --ci --coverage --reporters=default --reporters=jest-junit && node ./bin/inlineCoverageReportCSS.js",
    "analyze": "source-map-explorer build/static/js/main*",
    "analyze-vendor": "source-map-explorer build/static/js/1*",
    "eject": "craco eject",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "pre-commit-prettier": "pretty-quick --staged",
    "eslint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "deploy": "run-s build deploy-build",
    "deploy-production": "run-s build deploy-build-production",
    "deploy-build-debug": "node --inspect ./node_modules/.bin/ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./build/**\" \"/Apps/Survey\"",
    "deploy-build": " ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./build/**\" \"/Apps/Survey\" \"dev\"",
    "deploy-build-production": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./build/**\" \"/Apps/Survey\" \"production\"",
    "deploy-api": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./src/api/**\" \"/Apps/Survey\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@atlaskit/tooltip": "^15.1.3",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@baristalabs/craco-raw-loader": "^1.2.0",
    "@craco/craco": "^5.6.2",
    "@jackwilsdon/craco-use-babelrc": "^1.0.0",
    "@material-ui/core": "^4.7.1",
    "@material-ui/icons": "^4.5.1",
    "@navikt/craco-plugins": "^1.0.1",
    "@nivo/bullet": "^0.59.2",
    "@pnp/common": "^1.3.7",
    "@pnp/logging": "^1.3.5",
    "@pnp/odata": "^1.3.7",
    "@pnp/sp": "^1.3.7",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "16.9.14",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^3.0.8",
    "axios": "^0.19.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chart.js": "^2.9.3",
    "chartjs-lines-plugin": "^1.0.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-datalabels": "^0.7.0",
    "craco-less": "^1.14.4",
    "highcharts": "^7.2.1",
    "highcharts-react-official": "^2.2.2",
    "install": "^0.13.0",
    "lodash": "^4.17.15",
    "material-ui-flat-pagination": "^4.0.0",
    "mdbreact": "^4.23.0",
    "notistack": "^0.9.7",
    "npm": "^6.13.4",
    "prettier": "github:prettier/prettier#master",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.4",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.12.0",
    "react-jsx-highcharts": "^3.6.1",
    "react-loader-spinner": "^3.1.5",
    "react-promise-tracker": "^2.0.5",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-select": "^3.0.8",
    "react-super-select": "^1.0.23",
    "react-toastify": "^5.4.1",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "fs-jetpack": "^2.2.3",
    "metro-react-native-babel-preset": "^0.57.0",
    "npm-run-all": "^4.1.5",
    "sp-pnp-node": "^2.2.0",
    "ts-node": "^8.5.4"
  }
}
